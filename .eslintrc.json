{
    "extends": [
        "airbnb",
        "prettier",
        "prettier/react",
        "eslint-config-prettier"
    ],
    "parser": "babel-eslint",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "quotes": [1, "single", { "avoidEscape": true }],
        "prettier/prettier": ["error"]
    },
    "plugins": ["prettier"]
}
